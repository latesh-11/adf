
pipeline {
    agent any
     triggers {
        upstream(upstreamProjects: 'test/test_A/main', threshold: 'SUCCESS')
    }
    stages {
        stage('Hello') {
            steps {
                echo "Current branch: ${env.BRANCH_NAME}"
                echo "Build Cause: ${env.BUILD_CAUSE}"
                echo 'Hello World'
            }
        }
    }
}
